      <x:call function="els:isVideoUrl">
        <x:param name="url" select="'https://www.youtube.com/embed/GBZPKuBFedw?autoplay=1&amp;origin=http://example.com'"/>
      </x:call>
      <x:expect label="true" select="true()"/>
    </x:scenario>
    
    <x:scenario label="isVideoUrl : Vimeo">
